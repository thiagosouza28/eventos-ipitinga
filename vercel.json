{
  "version": 2,
  "framework": null,
  "installCommand": "npm install --prefix backend",
  "buildCommand": "npm run build --prefix backend",
  "builds": [
    {
      "src": "backend/dist/main.js",
      "use": "@vercel/node@2.15.15",
      "config": {
        "includeFiles": "backend/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/main.js"
    }
  ]
}

