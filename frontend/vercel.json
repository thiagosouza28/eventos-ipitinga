{
  "build": {
    "env": {
      "VITE_API_URL": "https://eventos-ipitinga.vercel.app/api",
      "VITE_APP_URL": "https://eventos-ipitinga.vercel.app"
    }
  },
  "env": {
    "VITE_API_URL": "https://eventos-ipitinga.vercel.app/api",
    "VITE_APP_URL": "https://eventos-ipitinga.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://18.231.250.231:3333/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "http://18.231.250.231:3333/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

